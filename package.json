{
  "name": "lit-ntml",
  "version": "2.16.1",
  "description": "Inspired by lit-html but for Node.js",
  "keywords": [
    "html",
    "lit-ntml",
    "minify",
    "npm",
    "ntml",
    "parse-html",
    "prettify",
    "tagged-template-literals",
    "template",
    "templating"
  ],
  "homepage": "https://github.com/motss/lit-ntml",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/lit-ntml.git"
  },
  "license": "MIT",
  "author": "Rong Sen Ng <wes.ngrongsen@gmail.com> (https://github.com/motss)",
  "main": "dist/index",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*.*js.map",
    "dist/*.*js",
    "dist/*.d.ts.map",
    "dist/*.d.ts"
  ],
  "scripts": {
    "build": "npm run clean && npm run rollup",
    "build:test": "npm run clean && tsc -p ./tsconfig.test.json",
    "clean": "shx rm -rf dist/",
    "jest": "jest --config ./.jestrc.json",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish",
    "prepublishOnly": "npm run lint && npm run build",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "npm run lint && npm run build:test && npm run jest",
    "ts": "npm run clean && tsc",
    "watch": "npm run ts -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && tsc --noEmit"
    }
  },
  "dependencies": {
    "parse5": "^6.0.1",
    "tslib": "^2.0.2"
  },
  "devDependencies": {
    "@reallyland/tsconfig": "^2.0.0",
    "@reallyland/tslint-config": "^1.1.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^14.11.8",
    "@types/parse5": "^5.0.3",
    "husky": "^4.3.0",
    "jest": "^24.9.0",
    "rollup": "^2.29.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tslint": "^0.2.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "shx": "^0.3.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
