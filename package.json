{
  "name": "lit-ntml",
  "version": "2.12.0",
  "description": "Inspired by lit-html but for Node.js",
  "keywords": [
    "html",
    "lit-ntml",
    "minify",
    "npm",
    "ntml",
    "parse-html",
    "prettify",
    "tagged-template-literals",
    "template",
    "templating"
  ],
  "homepage": "https://github.com/motss/lit-ntml",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/lit-ntml.git"
  },
  "license": "MIT",
  "author": "Rong Sen Ng <wes.ngrongsen@gmail.com> (https://github.com/motss)",
  "main": "dist/index",
  "module": "dist/lit-ntml.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*.*js.map",
    "dist/*.*js",
    "dist/*.d.ts.map",
    "dist/*.d.ts"
  ],
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.prod.cjs.json",
    "build:esm": "tsc -p ./tsconfig.prod.json",
    "build:prod": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:test": "tsc -p ./tsconfig.test.json",
    "clean": "shx rm -rf dist/ coverage/",
    "jest": "jest --config ./.jestrc.json",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish",
    "prepublishOnly": "npm run lint && npm run build",
    "rollup": "rollup -c ./rollup.config.js",
    "test": "npm run lint && npm run clean && npm run build:test && npm run jest",
    "ts": "tsc",
    "watch": "npm run ts -- --watch"
  },
  "dependencies": {
    "nodemod": "^2.4.0",
    "parse5": "^5.1.1",
    "tslib": "^1.11.0"
  },
  "devDependencies": {
    "@reallyland/tsconfig": "^2.0.0",
    "@reallyland/tslint-config": "^1.1.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.7.4",
    "@types/parse5": "^5.0.2",
    "jest": "^24.9.0",
    "shx": "^0.3.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
